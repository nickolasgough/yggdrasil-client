{
  "name": "Yggdrasil",
  "description": "Manages google meet tabs.",
  "version": "0.0.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "scripts/script.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
